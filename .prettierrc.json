{
  "singleQuote": true,
  "printWidth": 140,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "cursorOffset": -1,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxBracketSameLine": true,
  "jsxSingleQuote": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "semi": true,
  "tabWidth": 4,
  "useTabs": true,
  "requirePragma": false,
  "vueIndentScriptAndStyle": false
}
